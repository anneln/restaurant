<h1>Listing reviews</h1>

<table>
  <thead>
    <tr>
      <th>content</th>
      <th>rating</th>

      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @reviews.each do |review| %>
      <tr>
        <td><%= review.content %></td>
        <td><%= review.rating %></td>


      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Review', new_resto_review_path %>

